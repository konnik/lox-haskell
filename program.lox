var count = 5;
var previous = 0;
var current = 1;

for (var i=1; i <= count; i = i + 1) {
  print "Fibonacci number " + show i + " is " + show current + ".";

  // Calculate the next Fibonacci number
  var next = previous + current;
  previous = current;
  current = next;
}

var q = 1;
var korv = true;
while (korv) {
    print "In while loop...";
    
    q = q + 1;
    if (q>2) 
        korv = false;
}

